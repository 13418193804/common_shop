}

/* 箭头 */
.common-arrow {
  position: relative;
  width: 8px;
  height: 16px;
}
.common-arrow::after {
  content: " ";
  display: inline-block;
  width: 8px;
  height: 8px;
  position: absolute;
  top: 50%;
  left: 0;
  border-style: solid;
  border-color: #CCC;
  transform: translateY(-50%) matrix(0.71, 0.71, -.71, 0.71, 0, 0);
}
.common-arrow.right::after {
  left: -4px;
  border-width: 2px 2px 0 0;
}

.common-arrow.left::after {
  left: 3px;
  border-width: 0 0 2px 2px;
}
.common-arrow.color::after{
   border-color: #FFBB00;
}
.common-arrow.small {
  width: 6px;
  height: 12px;
}
.common-arrow.small::after {
  width: 6px;
  height: 6px;
}
.common-arrow.right.small::after {
  border-width: 1px 1px 0 0;
}
.common-arrow.left.small::after {
  border-width: 0 0 1px 1px;
}

.common-arrow.large {
  width: 10px;
  height: 20px;
}
.common-arrow.large::after {
  width: 10px;
  height: 10px;
}

/* 按钮 */
.kf-btn {
  position: relative;
