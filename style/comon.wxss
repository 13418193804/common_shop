/* 遮罩 */
.mask {
  position: fixed;
  z-index: 11;
  /* z-index: 1; */
	top: 0;
	left: 0;
	width: 100%;
	 height: 100vh; 
	background: rgba(0,0,0,0.70); 
  transition: all .35s ease-in-out;
}
.mask.hide {
  opacity: 0;
}

/* 弹窗 */
.popup {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 11;
  /* z-index: 2; */
  padding: 0 96rpx;
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
}
.popup.hide {
  display: none;
}
.popup .content {
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  width: 100%;
  height: 728rpx;
  border-radius: 16rpx;
  overflow: hidden;
  z-index: 12;
}
.popup .content .bg {
  width: 100%;
  height: 100%;
}
.popup .content .btn {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 48rpx;
  margin: 0 auto;
  z-index: 2;
  width: 294rpx;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  border-radius: 96rpx;
}

